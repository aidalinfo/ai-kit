{
  "name": "@ai_kit/server",
  "version": "1.0.5",
  "description": "HTTP server harness for AI Kit agents and workflows",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./instrument": {
      "import": "./dist/instrument.js",
      "types": "./dist/instrument.d.ts"
    }
  },
  "bin": {
    "server-kit": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "pnpm run clean && tsc -p tsconfig.build.json",
    "dev": "tsx src/cli.ts",
    "start": "node ./dist/cli.js",
    "test": "vitest run"
  },
  "keywords": [
    "ai",
    "workflow",
    "server"
  ],
  "license": "MIT",
  "packageManager": "pnpm@10.15.0",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.4",
    "@hono/swagger-ui": "^0.5.2",
    "dotenv": "^17.2.3",
    "hono": "^4.10.4",
    "jose": "^6.1.1",
    "openapi-types": "^12.1.3",
    "zod": "^3.1.12"
  },
  "peerDependencies": {
    "@ai_kit/core": ">=1.0.25 <2"
  },
  "devDependencies": {
    "@ai_kit/core": "workspace:^",
    "@types/node": "^24.7.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vitest": "^2.1.3"
  }
}
